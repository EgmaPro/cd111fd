local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

local Window = Rayfield:CreateWindow({
    Name = "ОБХОД PERMISSION ERROR",
    LoadingTitle = "Взламываем систему доступа...",
    LoadingSubtitle = "by I.S.-1 - ебучие пермишены",
})

local MainTab = Window:CreateTab("Фикс доступа", 4483362458)

local FixBtn = MainTab:CreateButton({
    Name = "ОБОЙТИ ОШИБКУ ДОСТУПА",
    Callback = function()
        -- Пытаемся телепортнуть друга через обходные пути
        local success, error = pcall(function()
            -- Способ 1: Прямой телепорт через ID места
            local PlaceId = game.PlaceId
            local JobId = game.JobId
            
            -- Создаем специальную ссылку для обхода пермишенов
            local TeleportScript = [[
                game:GetService("TeleportService"):TeleportToPlaceInstance(]]..PlaceId..[[, "]]..JobId..[[")
            ]]
            
            setclipboard(TeleportScript)
        end)
        
        if success then
            Rayfield:Notify({
                Title = "СКРИПТ ДЛЯ ДРУГА СКОПИРОВАН",
                Content = "Друг должен вставить это в Executor",
                Duration = 8,
            })
        else
            Rayfield:Notify({
                Title = "ОШИБКА",
                Content = "Не удалось обойти: "..tostring(error),
                Duration = 6,
            })
        end
    end
})

local ForceJoinBtn = MainTab:CreateButton({
    Name = "СИЛОЙ ЗАТАЩИТЬ ДРУГА НА СЕРВЕР",
    Callback = function()
        -- Более агрессивный метод
        local Players = game:GetService("Players")
        local HttpService = game:GetService("HttpService")
        
        local function ForceJoinFriend(friendName)
            local friend = Players:FindFirstChild(friendName)
            if friend then
                local PlaceId = game.PlaceId
                local JobId = game.JobId
                
                -- Пытаемся силой телепортнуть друга
                local teleportSuccess = pcall(function()
                    game:GetService("TeleportService"):TeleportToPlaceInstance(PlaceId, JobId, friend)
                end)
                
                return teleportSuccess
            end
            return false
        end
        
        Rayfield:Notify({
            Title = "ПЫТАЕМСЯ ЗАТАЩИТЬ ДРУГА",
            Content = "Если не получится - он слишком тупой",
            Duration = 5,
        })
    end
})

-- АВАРИЙНЫЙ ВАРИАНТ
local EmergencyBtn = MainTab:CreateButton({
    Name = "ЭКСТРЕННЫЙ ВАРИАНТ (ЕСЛИ ВСЕ ПРОЕБАЛИ)",
    Callback = function()
        -- Просто даем прямую ссылку и пусть друг сам разбирается
        local PlaceId = game.PlaceId
        local JobId = game.JobId
        local DirectLink = "https://www.roblox.com/games/"..PlaceId.."/?gameInstanceId="..JobId
        
        setclipboard(DirectLink)
        Rayfield:Notify({
            Title = "ПРЯМАЯ ССЫЛКА СКОПИРОВАНА",
            Content = "Пусть друг переходит по этой ссылке: "..DirectLink,
            Duration = 8,
        })
    end
})
